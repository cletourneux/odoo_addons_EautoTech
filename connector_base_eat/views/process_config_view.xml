<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="eat_process_config_form" model="ir.ui.view">
            <field name="name">Process Config Form</field>
            <field name="model">process.config.eat</field>
            <field name="arch" type="xml">
                <form string="Process Config">
                    <sheet>
                        <group>
                            <group>
                                <field name="active" invisible="1"/>
                                <field name="name"/>
                                <field name="instance_id"/>
                                <field name="endpoint_id"/>
                                <field name="business_type"/>
                                <field name="last_process_date"/>
                            </group>
                            <group>
                                <field name="process_type"/>
                                <field name="process_protocol"/>
                                <field name="process_channel"/>
                                <field name="process_way"/>
                                <field name="properties"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="eat_process_config_tree" model="ir.ui.view">
            <field name="name">Process Config Tree</field>
            <field name="model">process.config.eat</field>
            <field name="arch" type="xml">
                <list string="Process Config">
                    <field name="name"/>
                    <field name="endpoint_id"/>
                    <field name="process_protocol"/>
                    <field name="process_channel"/>
                    <field name="process_way"/>
                </list>
            </field>
        </record>

        <record id="eat_process_config_search" model="ir.ui.view">
            <field name="name">Process Config Search</field>
            <field name="model">process.config.eat</field>
            <field name="type">search</field>
            <field name="arch" type="xml">
                <search string="Process Config">
                    <field name="name"/>
                    <filter name="archive" string="Archived" domain="[('active','=',False)]"/>
                </search>
            </field>
        </record>

        <record id="eat_process_config_action" model="ir.actions.act_window">
            <field name="name">Process Config</field>
            <field name="res_model">process.config.eat</field>
            <field name="view_id" ref="eat_process_config_tree"/>
		    <field name="search_view_id" ref="eat_process_config_search"/>
            <field name="view_mode">list,form</field>
        </record>
    </data>
</odoo>